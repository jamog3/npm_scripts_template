{
  "name": "projectName",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "rimraf ./.temp && npm run watch",
    "build": "rimraf ./build && gulp build",
    "watch": "npm-run-all --parallel watch:*",
    "watch:html": "pug src/html/[!_]*.pug --basedir src/html/ --obj \"{\"rootPath\": \"/src/\"}\" --watch --pretty --out .temp/",
    "watch:sass": "sass --load-path=node_modules --watch *.sass:*.css src/stylesheets/:.temp/assets/stylesheets/ --style expanded",
    "watch:server": "browser-sync start --server ./.temp/ --startPath /"
  },
  "dependencies": {
    "bootstrap": "^4.1.3"
  },
  "devDependencies": {
    "browser-sync": "^2.24.6",
    "npm-run-all": "^4.1.3",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.6.2",
    "sass": "^1.10.3"
  }
}
